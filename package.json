{
  "name": "admin-front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/types": "^5.1.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.67",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "axios": "^0.21.1",
    "history": "^5.0.0",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-test-renderer": "^17.0.1",
    "styled-components": "5.1.1",
    "typescript": "^4.1.5",
    "zod": "^3.0.0-alpha.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jest-environment-jsdom-sixteen",
    "testAll": "react-app-rewired test --watchAll --coverage --env=jest-environment-jsdom-sixteen",
    "eject": "react-app-rewired eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src --ext .ts,.tsx"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.7.1",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "^2.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{tsx}",
      "!src/**/*.d.ts",
      "!<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "^@assets(.*)$": "<rootDir>/src/assets$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@constants(.*)$": "<rootDir>/src/constants$1",
      "^@contexts(.*)$": "<rootDir>/src/contexts$1",
      "^@hooks(.*)$": "<rootDir>/src/hooks$1",
      "^@integration(.*)$": "<rootDir>/src/integration$1",
      "^@interfaces(.*)$": "<rootDir>/src/interfaces$1",
      "^@pages(.*)$": "<rootDir>/src/pages$1",
      "^@services(.*)$": "<rootDir>/src/services$1",
      "^@styles(.*)$": "<rootDir>/src/styles$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@test-utils(.*)$": "<rootDir>/src/test-utils$1"
    }
  }
}
