{
  "name": "admin-front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.67",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-test-renderer": "^16.13.1",
    "styled-components": "5.1.1",
    "typescript": "^3.8.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jest-environment-jsdom-sixteen",
    "testAll": "react-app-rewired test --watchAll --env=jest-environment-jsdom-sixteen",
    "eject": "react-app-rewired eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src --ext .ts,.tsx"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint-config-airbnb-typescript": "^6.3.2",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "^2.1.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@pages(.*)$": "<rootDir>/src/pages$1",
      "^@assets(.*)$": "<rootDir>/src/assets$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@styled(.*)$": "<rootDir>/src/styled$1",
      "^@constants(.*)$": "<rootDir>/src/constants$1",
      "^@hooks(.*)$": "<rootDir>/src/hooks$1"
    }
  }
}
